{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "0be0d916-c59e-4806-aa4b-1975f122722d": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% if product.variants.first.inventory_quantity < 11 %}\n\n<p>\nWe have {{ product.variants.first.inventory_quantity}} items remaining\n<\/p>\n\n{% endif %}"
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "4ab78acd-f063-4c21-b79c-0595f89aa1ee": {
          "type": "custom_liquid",
          "disabled": true,
          "settings": {
            "custom_liquid": "<label for=\"message\" class=\"form__label\" >Message:<\/label>\n\n<div class=\"field\">\n\n<textarea id=\"message\" placeholder=\"Message\"\nname=\"properties[message]\"\nform=\"product-form-{{ section.id }}\" class=\"text-area field__input\" >\n<\/textarea>\n\n<\/div>"
          }
        },
        "bc2a1659-f0ee-4d80-9430-6d6fd8e636a9": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% if product.selected_or_first_available_variant.sku != blank %}\n\n<p style=\"margin-top: -15px\";> SKU: {{ product.selected_or_first_available_variant.sku\n}}<\/p>\n\n{% endif %}"
          }
        },
        "871cfae2-d755-44d1-af95-424cde51a75b": {
          "type": "inventory_count",
          "settings": {
            "show_inventory_threshold": 0,
            "text_color": "#dd1d1d"
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true,
            "show_gift_card_recipient": false
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "848327f1-000f-4a7e-9682-ed89cc4c85f1": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "TEST\n<ul style=\"justify-content: center; class=\"list list-payment\" role=\"list\">\n              {%- for type in shop.enabled_payment_types -%}\n                <li class=\"list-payment__item\">\n                  {{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}\n                <\/li>\n              {%- endfor -%}\n            <\/ul>"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "price",
        "0be0d916-c59e-4806-aa4b-1975f122722d",
        "variant_picker",
        "quantity_selector",
        "4ab78acd-f063-4c21-b79c-0595f89aa1ee",
        "bc2a1659-f0ee-4d80-9430-6d6fd8e636a9",
        "871cfae2-d755-44d1-af95-424cde51a75b",
        "buy_buttons",
        "description",
        "share",
        "848327f1-000f-4a7e-9682-ed89cc4c85f1"
      ],
      "settings": {
        "enable_sticky_info": true,
        "media_size": "large",
        "constrain_to_viewport": true,
        "media_fit": "contain",
        "gallery_layout": "stacked",
        "media_position": "left",
        "image_zoom": "lightbox",
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "background-1",
        "image_ratio": "square",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 28
      }
    }
  },
  "order": [
    "main",
    "related-products"
  ]
}